div(class="modal fade" id=`exampleModal${Sno}` tabindex="-1" role="dialog" aria-labelledby=`exampleModalLabel${Sno}`  aria-hidden="true")
    div(class="modal-dialog" role="document")
        div(class="modal-content")
            div(class="modal-header")
                h5(class="modal-title" id=`exampleModalLabel${Sno}`)= "Bhandari Path Labs"
                button(type="button" class="close" data-dismiss="modal" aria-label="Close")
                    <span aria-hidden="true">&times</span>
                
            div(class="modal-body")
                h5= "Do you want to delete this item"

            div(class="modal-footer")
                button(type="button" class="btn btn-secondary" data-dismiss="modal")= "No"
                form(action=`${link}` method="POST")
                    button(type="submit" class="btn btn-primary")= "Yes"